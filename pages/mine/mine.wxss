/* pages/mine/mine.wxss */
page{
  height: 100%;
  background-image: url("data:image/jpeg;base64,")
}
.menu{
  width: 88%;
  background-color: #fff;
  margin: 0px 14px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-radius: 10px;
  padding: 0px 10px;
  border: 1px #aaa solid
}
.userinfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 10px 30px;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}
.user_name{
  color: #aaa;
  flex-direction: column;
  display: flex;
  align-items: left;
  margin-left: 10px
}
.menu_name{
  padding: 10px 15px;
  font-size: 18px;
  font-weight: bolder;
}
.menu_sty{
  padding: 8px 0px
}
.infome{
  margin: 18px 14px;
  padding: 5px 25px;
  border-top: solid 2px #aaa;
  justify-content: space-between;
  display: flex
}
